{
  "name": "feedback",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build": "blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "migrate:dev": "blitz prisma migrate dev --preview-feature",
    "migrate:prod": "blitz prisma migrate deploy --preview-feature",
